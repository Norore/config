let g:zenburn_high_Contrast=1                                                                                                                                                          
let g:zenburn_transparent=1
colors zenburn
syntax on
set number
set ruler
set cursorline
"set textwidth=80
set tabstop=4
set shiftwidth=4
set smartindent
set list
set listchars:tab:\|\ 

set foldmethod=manual
autocmd BufNewFile,BufRead *.json set ft=javascript
